         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.birt.report.data.oda.sampledb"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.birt.report.data.oda.sampledb.ui"
         download-size="0"
         install-size="0"
