<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.birt.example"
      label="Eclipse BIRT Example"
      version="2.1.0.qualifier"
      provider-name="Eclipse.org"
      plugin="org.eclipse.birt.example">

   <description url="http://www.eclipse.org/birt">
      %description
   </description>

   <copyright url="http://www.example.com/copyright">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="Business Intelligence and Reporting Tools (BIRT) Updates" url="http://download.eclipse.org/birt/update-site"/>
      <discovery label="BIRT update site" url="http://download.eclipse.org/birt/update-site"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui"/>
      <import feature="org.eclipse.datatools.connectivity.oda.designer.feature" version="0.9.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.birt.chart" version="2.1.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.birt.chart.runtime" version="2.1.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.birt.report.designer" version="2.1.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.birt.report.runtime" version="2.1.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.eclipse.birt.chart.examples"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.birt.report.designer.example.samplereport"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.birt.report.data.oda.sampledb"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.birt.report.data.oda.sampledb.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.birt.example"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

</feature>
